# .env.example
# Rename this file to .env and fill in the values

# Langfuse 
LANGFUSE_SECRET_KEY="your_langfuse_secret_key"
LANGFUSE_PUBLIC_KEY="your_langfuse_public_key"
LANGFUSE_HOST="your_langfuse_host"

# Azure OpenAI GPT-4o Mini
AZURE_OPENAI_GPT_4O_MINI_API_KEY="your_azure_openai_gpt_4o_mini_api_key"
AZURE_OPENAI_GPT_4O_MINI_ENDPOINT="your_azure_openai_gpt_4o_mini_endpoint"
AZURE_OPENAI_GPT_4O_MINI_API_VERSION="your_azure_openai_gpt_4o_mini_api_version"
AZURE_OPENAI_GPT_4O_MINI_DEPLOYMENT_NAME="your_azure_openai_gpt_4o_mini_deployment_name"

# OpenAI API Key
# OPENAI_API_KEY="your_openai_api_key"

# Anthropic API Key
# ANTHROPIC_API_KEY="your_anthropic_api_key"

# Hugging Face API Key (if using hosted models)
# HUGGINGFACE_API_KEY="your_huggingface_api_key"

# Other environment variables
# PYTHONPATH="$(pwd)"